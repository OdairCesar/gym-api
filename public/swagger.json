{
  "openapi": "3.0.0",
  "info": {
    "title": "Gym API",
    "description": "API para gerenciamento de academias, usuários, treinos, dietas e produtos",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:3333",
      "description": "Servidor de desenvolvimento"
    }
  ],
